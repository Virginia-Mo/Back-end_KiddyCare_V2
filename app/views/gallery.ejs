<a href="/login">Log in</a>
<a href="/classes" class="joinclass__btn">Join Class</a>
</header>
<main class="maincontent">
    <section class="banner banner__pages">
        <div class="banner__part1">
            <h1 class="banner__title">Gallery</h1>
            <p class="banner__description banner__description--pages"><a href="index.html" class="banner__link">
                    Home</a> / Our Gallery</p>
        </div>
    </section>
    <div class="container">
        <section class="gallerylist">
            <h3 class="content__subtitle content__subtitle--border">Our gallery</h3>
            <h2 class="content__title content__title--center">Our Kids School Gallery</h2>
            <div class="gallery__container">
            </div>
        </section>
        <section class="gallery__buttonsbox">
            <button class="joinclass__btn joinclass__btn--gallery" id="all">All</button>
            <button class="joinclass__btn joinclass__btn--gallery" id="playingbtn">Playing</button>
            <button class="joinclass__btn joinclass__btn--gallery" id="drawingbtn">Drawing</button>
            <button class="joinclass__btn joinclass__btn--gallery" id="readingbtn">Reading</button>
        </section>
        <section class="photos__container">
            <div id="galleryphotos" class="animate__animated animate__zoomIn">
                <img src="../img/portfolio-1.jpg" alt="img1" class="gallery-img">
                <img src="../img/portfolio-2.jpg" alt="img2" class="gallery-img">
                <img src="../img/portfolio-3.jpg" alt="img3" class="gallery-img">
                <img src="../img/portfolio-4.jpg" alt="img4" class="gallery-img">
                <img src="../img/portfolio-5.jpg" alt="img5" class="gallery-img">
                <img src="../img/portfolio-6.jpg" alt="img6" class="gallery-img">
            </div>
            <div id="allphotos" class="hide animate__animated  animate__fadeInUp">
                <img src="../img/portfolio-1.jpg" alt="img1" class="gallery-img">
                <img src="../img/portfolio-2.jpg" alt="img2" class="gallery-img">
                <img src="../img/portfolio-3.jpg" alt="img3" class="gallery-img">
                <img src="../img/portfolio-4.jpg" alt="img4" class="gallery-img">
                <img src="../img/portfolio-5.jpg" alt="img5" class="gallery-img">
                <img src="../img/portfolio-6.jpg" alt="img6" class="gallery-img">
            </div>
            <div id="playing" class="hide animate__animated animate__fadeInRight">
                <img src="../img/portfolio-1.jpg" alt="img1" class="gallery-img">
                <img src="../img/portfolio-3.jpg" alt="img3" class="gallery-img">
            </div>
            <div id="drawing" class="hide animate__animated animate__fadeInUp">
                <img src="../img/portfolio-2.jpg" alt="img2" class="gallery-img">
                <img src="../img/portfolio-4.jpg" alt="img4" class="gallery-img">
            </div>
            <div id="reading" class="hide animate__animated animate__fadeInRight">
                <img src="../img/portfolio-5.jpg" alt="img5" class="gallery-img">
                <img src="../img/portfolio-6.jpg" alt="img6" class="gallery-img">
            </div>

        </section>
    </div>
</main>
